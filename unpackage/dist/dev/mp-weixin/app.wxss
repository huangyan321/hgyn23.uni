
	/*每个页面公共css */
page {
		box-sizing: border-box;
		display: flex;
		width: 100%;
		min-height: 100%;
}
@font-face {
		font-family: 'iconfont';
		src: url('data:font/ttf;base64,') format('truetype');
}
.iconfont {
		font-family: "iconfont" !important;
		font-size: 16px;
		font-style: normal;
		-webkit-font-smoothing: antialiased;
		-moz-osx-font-smoothing: grayscale;
}

